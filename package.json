{
  "name": "obsidian-plugin-e2e-test",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:rollup && yarn build:deploy",
    "build:rollup": "rollup --config rollup.config.mjs --environment BUILD:production",
    "build:deploy": "mkdir -p test/empty_vault/.obsidian/plugins/sample-plugin && cp manifest.json styles.css dist/main.js test/empty_vault/.obsidian/plugins/sample-plugin",
    "test": "yarn playwright test --update-snapshots --headed",
    "check": "yarn cspell --no-cache --unique .",
    "cspell:update": "cspell --no-cache --words-only --unique . | sort -uf >./.vscode/cspell-obsidian-e2e.txt",
    "format": "prettier --write ."
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@4.2.2+sha512.c44e283c54e02de9d1da8687025b030078c1b9648d2895a65aab8e64225bfb7becba87e1809fc0b4b6778bbd47a1e2ab6ac647de4c5e383a53a7c17db6c3ff4b",
  "dependencies": {
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.26.3",
    "electron": "^30.0.3",
    "electron-chromedriver": "^29.3.3",
    "obsidian": "^1.5.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@playwright/test": "^1.44.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.12.11",
    "@types/selenium-webdriver": "^4",
    "cspell": "^8.8.0",
    "eslint": "^9.2.0",
    "globals": "^15.2.0",
    "playwright": "^1.44.0",
    "prettier": "^3.2.5",
    "rollup": "^4.17.2",
    "selenium-webdriver": "^4.20.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  }
}
